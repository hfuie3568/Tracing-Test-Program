<Window x:Class="Tracing_Test_Program.Result"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tracing_Test_Program"
        mc:Ignorable="d"
        Title="Result" Height="300" Width="300">
    <Grid>
        <Label x:Name="label" Content="당신의 점수는?" HorizontalAlignment="Left" Margin="34,25,0,0" VerticalAlignment="Top" Height="61" Width="228" FontSize="32"/>
        <Label x:Name="result" Content="" HorizontalAlignment="Left" Margin="34,149,0,0" VerticalAlignment="Top" Height="83" Width="228" FontSize="26.667" BorderThickness="0" HorizontalContentAlignment="Center"/>
        <Button x:Name="b" Content="확인" HorizontalAlignment="Left" Margin="231,90,0,0" VerticalAlignment="Top" Width="41" Height="30" Click="b_Click">
            <Button.Effect>
                <DropShadowEffect BlurRadius="2" Opacity="0.5" RenderingBias="Quality" ShadowDepth="3"/>
            </Button.Effect>
        </Button>

    </Grid>
</Window>
